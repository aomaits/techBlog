    <main>
      <section>
        {{#each posts as |post| }}
        <article>
          <h2>{{post.title}} </h2>
          <p id="createdByLine">Created by {{post.user.name}} on {{post.date_created}}</p>
          <div>
            <p>
              {{post.content}}
            </p>
          </div>
        </article>
        {{/each}}
      </section>
    </main>